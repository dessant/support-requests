{
  "name": "support-requests",
  "version": "0.2.0",
  "description": "A GitHub App built with Probot that comments on and closes issues labeled as support requests.",
  "author": "Armin Sebastian",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/dessant/support-requests.git",
    "type": "git"
  },
  "bugs": "https://github.com/dessant/support-requests/issues",
  "main": "src/index.js",
  "scripts": {
    "start": "probot run ./src/index.js",
    "watch": "nodemon --exec 'npm start'",
    "update": "ncu --upgrade --upgradeAll && yarn",
    "push": "git push --follow-tags origin master",
    "release": "standard-version && yarn run push"
  },
  "files": [
    "docs",
    "src"
  ],
  "dependencies": {
    "joi": "^13.2.0",
    "probot": "^6.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.3",
    "npm-check-updates": "^2.14.2",
    "smee-client": "^1.0.1",
    "standard-version": "^4.3.0"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  },
  "keywords": [
    "github",
    "issues",
    "support",
    "label",
    "support issues",
    "label issues",
    "close issues",
    "automation",
    "github app",
    "probot",
    "probot app",
    "bot"
  ]
}
